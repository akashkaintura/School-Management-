# Database
DATABASE_URL="postgresql://user:password@host:5432/schooldb"

# JWT
JWT_SECRET="your-super-secret-jwt-key"
JWT_EXPIRATION="1d"
JWT_REFRESH_SECRET="your-refresh-secret"
